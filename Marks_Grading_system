#include<stdio.h>
main(){
float a;
printf("Enter your marks: ");
scanf("%f", &a);
if(a>=85 && a<=100)
	printf("Your Grade is: A"); // Grade A

	else if(a>=70 && a<=84)
	printf("Your Grade is: B"); // Grade B
	
	else if(a>=55 && a<=69)
	printf("Your Grade is: C"); // Grade C
	
	else if(a>=40 && a<=54)
	printf("Your Grade is: D"); // Grade D
	
	else if(a<40)
	printf("Your Grade is: F"); // Grade F
	
	else printf("Your marks should be within 100, maybe try percentage?, but make sure just to give the number and not the percentage sign.");
}
